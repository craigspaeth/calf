{
  "name": "calf",
  "version": "1.0.0",
  "description": "The original monolithic codebase for AdRhino",
  "main": "index.js",
  "scripts": {
    "start": "concurrently 'mongod' 'npm run server' 'npm run lint'",
    "server": "forever -c 'npm run node' index.js",
    "node": "NODE_PATH=$NODE_PATH:./lib node -r dotenv/config -r babel-core/register -r bluebird-register",
    "lint": "nodemon --exec 'standard | notify'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrhino/calf.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adrhino/calf/issues"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "homepage": "https://github.com/adrhino/calf#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "auth0-lock": "^9.0.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babelify": "^7.2.0",
    "baobab": "^2.3.3",
    "bluebird": "^3.3.5",
    "brfs": "^1.4.3",
    "brfs-babel": "^1.0.0",
    "browserify": "^13.0.0",
    "browserify-dev-middleware": "^1.0.0",
    "concurrently": "^2.0.0",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "envify": "^3.4.0",
    "forever": "^0.15.1",
    "graphiql": "^0.7.1",
    "graphql": "^0.6.0",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^8.0.5",
    "joi-objectid": "^2.0.0",
    "koa": "^2.0.0-alpha.3",
    "koa-bodyparser": "^3.0.0",
    "koa-browserify-middleware": "^1.0.0",
    "koa-convert": "^1.2.0",
    "koa-generic-session": "^1.6",
    "koa-graphql": "^0.5.1",
    "koa-jwt": "^1.1.2",
    "koa-mount": "git://github.com/coderhaoxin/koa-mount.git#next-by-promise",
    "koa-passport": "^2.0.1",
    "koa-route": "^3.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^2.0.0",
    "lodash": "^4.7.0",
    "moment": "^2.12.0",
    "node-notifier": "^4.5.0",
    "nodemon": "^1.9.1",
    "npm": "^3.8.2",
    "page": "^1.7.1",
    "passport": "^0.3.2",
    "passport-auth0": "^0.5.1",
    "pluralize": "^2.0.0",
    "promised-mongo": "^1.2.0",
    "radium": "^0.17.0",
    "react": "^15.0.2",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.0.2",
    "react-functional": "^1.2.0",
    "react-tag-input": "2.2.6 || >= 2.3.0",
    "rewire": "^2.5.1",
    "standard": "^7.0.1",
    "standard-format": "^2.1.1"
  }
}
